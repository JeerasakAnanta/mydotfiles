[global_config]
[keybindings]
  close_term = <Alt>q
  switch_to_tab_1 = <Alt>1
  switch_to_tab_2 = <Alt>2
  switch_to_tab_3 = <Alt>3
  switch_to_tab_4 = <Alt>4
  switch_to_tab_10 = <Alt>0
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    font = Source Code Pro 12
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
